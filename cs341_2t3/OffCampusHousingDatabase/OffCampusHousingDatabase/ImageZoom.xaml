<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="OffCampusHousingDatabase.ImageZoom"
        Title="Rent Oshkosh" Height="450" Width="800" Background="White" WindowStyle="ThreeDBorderWindow" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid HorizontalAlignment="Left" Height="450" Width="800" Margin="0,0,0,0" VerticalAlignment="Top">
        <Rectangle Fill="#FF90BCFD" HorizontalAlignment="Left" Height="46" Stroke="Black" VerticalAlignment="Top" Width="818" Margin="-18,0,0,0"/>

        <Image x:Name="ImageBox" Margin="65,51,65,40"/>
        <Button x:Name="nextImageButton" Content="&gt;" HorizontalAlignment="Right" Margin="0,199,35,0" VerticalAlignment="Top" Width="25" Height="25" Click="nextImageButton_Click" IsEnabled="False"/>
        <Button x:Name="prevImageButton" Content="&lt;" HorizontalAlignment="Left" Margin="10,199,0,0" VerticalAlignment="Top" Width="25" Height="25" Click="prevImageButton_Click" IsEnabled="False"/>

    </Grid>
</Window>
